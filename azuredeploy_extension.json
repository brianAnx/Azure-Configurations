{
    "extensionProfile": {
      "extensions": [
        {
          "name": "AppInstall",
          "properties": {
            "publisher": "Microsoft.Azure.Extensions",
            "type": "CustomScript",
            "typeHandlerVersion": "1.1",
            "autoUpgradeMinorVersion": true,
            "settings": {
              "fileUris": [
                "https://raw.githubusercontent.com/brianAnx/Azure-Configurations/main/vsbuildinstall.ps1"
              ],
              "commandToExecute": "[concat('powershell -ExecutionPolicy Unrestricted -File ', './vsbuildinstall.ps1')]"
            }
          }
        }
      ]
    }
  }
  
  